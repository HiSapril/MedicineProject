{
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        }
      },
      "appointment-route": {
        "ClusterId": "appointment-cluster",
        "Match": {
          "Path": "/api/appointments/{**catch-all}"
        }
      },
      "medication-route": {
        "ClusterId": "medication-cluster",
        "Match": {
          "Path": "/api/medications/{**catch-all}"
        }
      },
      "health-route": {
        "ClusterId": "health-cluster",
        "Match": {
          "Path": "/api/health-logs/{**catch-all}"
        }
      },
      "reminder-route": {
        "ClusterId": "reminder-cluster",
        "Match": {
          "Path": "/api/reminders/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth": {
            "Address": "http://localhost:5004/"
          }
        }
      },
      "appointment-cluster": {
        "Destinations": {
          "appointment": {
            "Address": "http://localhost:5001/"
          }
        }
      },
      "medication-cluster": {
        "Destinations": {
          "medication": {
            "Address": "http://localhost:5002/"
          }
        }
      },
      "health-cluster": {
        "Destinations": {
          "health": {
            "Address": "http://localhost:5003/"
          }
        }
      },
      "reminder-cluster": {
        "Destinations": {
          "reminder": {
            "Address": "http://localhost:5005/"
          }
        }
      }
    }
  }
}